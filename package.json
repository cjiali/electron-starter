{
  "name": "electron-starter",
  "version": "0.1.0",
  "private": true,
  "main": "build/main.js",
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "concurrently yarn:start:*",
    "prestart": "cross-env NODE_ENV=development yarn gulp",
    "start:renderer": "cross-env NODE_ENV=development BROWSER=none react-app-rewired start",
    "start:watch": "cross-env NODE_ENV=development yarn gulp watch",
    "start:main": "wait-on http://localhost:3000 && nodemon --watch ./build --exec yarn electron:dev",
    "build": "yarn build:renderer && yarn build:main",
    "build:renderer": "cross-env NODE_ENV=production react-app-rewired build",
    "build:main": "cross-env NODE_ENV=production yarn gulp build",
    "electron:dev": "cross-env NODE_ENV=development electron . --no-sandbox",
    "electron:prod": "cross-env NODE_ENV=production electron . --no-sandbox",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write . --ignore-unknown"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/",
      "package.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/verify-commit-msg.js ${HUSKY_GIT_PARAMS}"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "del": "^6.0.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "wait-on": "^5.2.1"
  }
}
